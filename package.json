{
  "name": "cleancity-waste-reporting",
  "version": "2.0.0",
  "description": "CleanCity - A comprehensive waste reporting system with geocoding, driver assignment, and interactive maps for urban cleanliness management",
  "keywords": [
    "waste-management",
    "reporting-system",
    "geocoding",
    "urban-cleanliness",
    "environmental",
    "driver-assignment",
    "maps",
    "mongodb",
    "express",
    "nodejs"
  ],
  "license": "ISC",
  "author": {
    "name": "Ismail Aj",
    "email": "contact@cleancity.com"
  },
  "contributors": [
    {
      "name": "Enhanced Reporting Team",
      "email": "dev@cleancity.com"
    }
  ],
  "type": "module",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "serve": "node server.js",
    "test": "node scripts/test-connection.js",
    "test:geocoder": "node scripts/test-geocoder.js",
    "test:status": "node scripts/testStatusUpdate.js",
    "admin:create": "node scripts/createAdmin.js",
    "admin:verify": "node scripts/verifyAdminAccess.js",
    "db:test": "node scripts/test-connection.js",
    "db:seed": "node scripts/seed.js",
    "db:clear": "node scripts/seed.js --clear-only",
    "setup": "npm install && npm run db:test",
    "setup:full": "npm install && npm run db:test && npm run db:seed",
    "lint": "echo 'Linting not configured yet'",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cleancity/waste-reporting-system.git"
  },
  "bugs": {
    "url": "https://github.com/cleancity/waste-reporting-system/issues"
  },
  "homepage": "https://github.com/cleancity/waste-reporting-system#readme",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.20.0",
    "multer": "^1.4.5-lts.1",
    "node-geocoder": "^4.4.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  },
  "optionalDependencies": {
    "sharp": "^0.33.2"
  },
  "config": {
    "port": 5050,
    "mongoTimeout": 10000
  },
  "directories": {
    "lib": "./utils",
    "test": "./scripts"
  },
  "files": [
    "server.js",
    "config/",
    "middleware/",
    "models/",
    "routes/",
    "utils/",
    "public/",
    "scripts/",
    "README.md"
  ]
}
